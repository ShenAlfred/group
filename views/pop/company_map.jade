.mapmask
    .main
        each map in content.companyList
            .maskbig
                span.mapclose X
                .maskmap
                    iframe(src="/symap?h=#{popmapHeight}&w=#{popmapWidth}&mapPoint=#{map.mapPoint}" height="#{popmapHeight}px" width="#{popmapWidth}px" scrolling="no" style="background-color: transparent;" marginwidth="0" marginheight="0" frameborder="0")
                .maskmapline
                .maskmap-content
                    .maskmaptitle #{map.name}
                    .maskmap-address    Address: #{map.address}
                    .maskmap-tel    TEL:   #{map.telephone}
                    .maskmap-fax    FAX:   #{map.fax}